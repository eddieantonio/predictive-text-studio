{
  "include": [
    "src/worker/**/*",
    "test/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "lib": ["ES5", "ES6", "WebWorker"],
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@worker/*": ["src/worker/*"],
      "*": ["node_modules/*", "types/*"]
    },
    "allowJs": true,
    "noImplicitAny": false

  }
}
