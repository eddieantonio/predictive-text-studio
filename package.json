{
  "name": "predictive-text-studio",
  "version": "2020.9.1-alpha",
  "description": "Create predictive text for YOUR language",
  "main": "index.js",
  "repository": "git@github.com:eddieantonio/predictive-text-studio.git",
  "author": "Eddie Antonio Santos <easantos@ualberta.ca>",
  "license": "ISC",
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "cy": "cypress open",
    "dev": "rollup -c -w",
    "lint": "eslint '{src,test,cypress}/**/*.ts'",
    "lint:fix": "eslint --fix '{src,test,cypress}/**/*.ts'",
    "prettier": "prettier --write '{src,test,cypress}/**/*.{ts,svelte}'",
    "start": "sirv public -s",
    "test": "ava",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.10",
    "@types/bcp-47": "^1.0.0",
    "@types/node": "^14.11.8",
    "@types/node-fetch": "^2.5.7",
    "@types/sinon": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "ava": "^3.12.1",
    "core-js-bundle": "^3.6.4",
    "cypress": "5.0.0",
    "cypress-file-upload": "^4.1.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fake-indexeddb": "^3.1.2",
    "husky": "^4.3.0",
    "nock": "^13.0.4",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.1",
    "prettier-plugin-svelte": "^1.4.0",
    "regenerator-runtime": "^0.13.5",
    "rollup": "^2.26.5",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^7.0.0",
    "sinon": "^9.1.0",
    "sirv-cli": "^1.0.6",
    "svelte": "^3.24.1",
    "svelte-check": "^1.0.23",
    "svelte-preprocess": "^4.1.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9"
  },
  "dependencies": {
    "@predictive-text-studio/lexical-model-compiler": "^14.0.0-alpha.0",
    "bcp-47": "^1.0.7",
    "comlink": "^4.3.0",
    "dexie": "^3.0.2",
    "exceljs": "~3.5.0",
    "jszip": "^3.5.0",
    "svelte-routing": "^1.4.2",
    "tslib": "^2.0.1"
  },
  "ava": {
    "files": [
      "test/**/test-*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint:fix && yarn run prettier"
    }
  }
}
